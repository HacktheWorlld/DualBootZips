show_progress(0.0, 0);

ui_print("---------------------");
ui_print("Wipe secondary /cache");
ui_print("---------------------");
run_program("/sbin/busybox", "umount", "/cache");
run_program("/sbin/busybox", "mount", "/cache");

show_progress(0.33, 0);

ui_print("Deleting /cache/dual ...");
delete_recursive("/cache/dual");

show_progress(0.66, 0);

unmount("/cache");

show_progress(1.0, 0);

ui_print("Successfully wiped secondary /cache");
